Given nix (attribute):
  {
    foo = pkgs.callPackage ./examples/foo {};
  }

Do (reindent):
  vip=

Expect (indentation):
~~~~~~~
  {
    foo = pkgs.callPackage ./examples/foo {};
  }
~~~~~~~

Execute (syntax):
  AssertEqual SyntaxOf('foo'), 'nixAttribute'

Given nix (list):
  {
    foo = [
      a
      b
    ]
  }

Do (reindent):
  vip=

Expect (indentation):
~~~~~~~
  {
    foo = [
      a
      b
    ]
  }
~~~~~~~

Execute (syntax):
  AssertEqual SyntaxOf('foo'), 'nixAttribute'

Given nix (comment):
  let
    foo = true;
    bar = false;
  in foo

Do (reindent):
  vip=

Expect (let):
~~~~~~~
  let
    foo = true;
    bar = false;
  in foo
~~~~~~~

Execute (syntax):
  AssertEqual SyntaxOf('let'), 'nixKeyword'
  AssertEqual SyntaxOf('in'), 'nixKeyword'
  AssertEqual SyntaxOf('foo'), 'nixAttribute'
  AssertEqual SyntaxOf('bar'), 'nixAttribute'
